<Window x:Class="dz_26._10.Reg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dz_26._10"
        mc:Ignorable="d"
                Title="Registration" Height="250" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <DockPanel LastChildFill="False" Grid.Row="0">
            <Label Content="Enter key:" FontSize="25" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="20" DockPanel.Dock="Left"/>
            <TextBox Name ="LoginTxtBlock" FontSize="20" Margin="20" Width="300" Height="30" DockPanel.Dock="Right"/>
        </DockPanel>
        <DockPanel LastChildFill="False" HorizontalAlignment="Center" Margin="0,16,0,21" Grid.Row="1">
            <Button Name="LogB" Content="Register" Command="{x:Static local:Reg.RegCommand}" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="10" Margin="10"/>
            <Button Name="CancleB" Content="Cancle" Command="{x:Static local:Reg.CancCommand}" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="10" Margin="10"/>
        </DockPanel>
    </Grid>
</Window>
